{
  "name": "rambler-ui",
  "version": "3.26.0",
  "description": "Rambler UI Components based on React.js",
  "main": "index.js",
  "scripts": {
    "test": "/usr/bin/env node --max-old-space-size=4096 ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS_custom",
    "karma": "/usr/bin/env node --max-old-space-size=4096 ./node_modules/karma/bin/karma start --single-run --browsers Chrome",
    "prebuild": "rm -rf build",
    "build": "NODE_ENV=production VERSION=$npm_package_version babel src --out-dir build --ignore **/*.test.js",
    "postbuild": "cp package.json .npmignore README.md build",
    "dev": "NODE_ENV=development webpack-dev-server -d --hot --config docs/webpack.config.js",
    "prepages": "rm -rf docs/build",
    "pages": "NODE_ENV=production webpack -p --progress --config docs/webpack.config.js && node docs/scripts/publish-site.js",
    "eslint": "eslint '**/*.js'",
    "size": "size-limit",
    "precommit": "lint-staged",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rambler.ru:rambler-ui/rambler-ui.git"
  },
  "author": "Rambler team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rambler-digital-solutions/rambler-ui/issues"
  },
  "homepage": "https://rambler-digital-solutions.github.io/rambler-ui",
  "dependencies": {
    "classnames": "^2.2.5",
    "deepmerge": "^2.1.0",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react-jss": "^8.3.5"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.9.0",
    "@mdx-js/mdx": "^0.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-preval": "^1.6.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-standalone": "^6.26.0",
    "color": "^1.0.3",
    "css-loader": "^0.28.11",
    "debug": "^2.2.0",
    "doctrine": "^2.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.7.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "get-md-title": "^1.0.4",
    "gh-pages": "^0.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.11.4",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.7",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "lint-staged": "^6.1.0",
    "minimist": "^1.2.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-codemirror": "^1.0.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.2.0",
    "react-highlight.js": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "recast": "^0.13.2",
    "remark": "^9.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^7.0.0",
    "size-limit": "^0.16.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "rambler&co"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
