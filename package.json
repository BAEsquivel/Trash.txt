{
  "name": "rambler-ui",
  "version": "3.15.1",
  "description": "Rambler UI Components based on React.js",
  "main": "index.js",
  "scripts": {
    "test": "/usr/bin/env node --max-old-space-size=4096 ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS_custom",
    "karma": "/usr/bin/env node --max-old-space-size=4096 ./node_modules/karma/bin/karma start --single-run --browsers Chrome",
    "prebuild": "rm -rf build",
    "build": "NODE_ENV=production VERSION=$npm_package_version babel src --out-dir build --ignore **/__doc__,**/__test__",
    "postbuild": "cp package.json .npmignore README.md build",
    "dev": "NODE_ENV=development playground server",
    "pages": "NODE_ENV=production playground pages --publish",
    "eslint": "eslint '**/*.js'",
    "size": "size-limit",
    "precommit": "lint-staged",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rambler.ru:rambler-ui/rambler-ui.git"
  },
  "author": "Rambler team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rambler-digital-solutions/rambler-ui/issues"
  },
  "homepage": "https://rambler-digital-solutions.github.io/rambler-ui",
  "ghPagesRepository": {
    "url": "git+https://github.com/rambler-digital-solutions/rambler-ui.git"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "deepmerge": "^2.1.0",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react-jss": "^8.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "color": "^1.0.3",
    "debug": "^2.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.7.2",
    "eslint-import-resolver-webpack": "^0.3.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.11.4",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lint-staged": "^6.1.0",
    "rambler-ui-playground": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.0.0",
    "size-limit": "^0.16.1",
    "webpack": "^1.13.2"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "rambler&co"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
